{
    "manifest_version": 2,
    "name": "Outlook-Bleacher",
    "version": "0.0.3",
    "description": "A chrome extension to 'clean up' outlook web, as per my tastes :)",
    "content_scripts": [
        {
            "css": [
                "styles.css"
            ],
            "matches": [
                "https://outlook.office365.com/mail/*",
                "https://outlook.office365.com/calendar/*"
            ]
        }
    ]
}